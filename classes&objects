 
public class classes_objects {     //This is a class 
  int x = 2;           // Class attribute
  final int y =4;    //Class attribute 
  public static void main(String[] args) {
    classes_objects object = new classes_objects();         //This is an object 
    
    classes_objects object2 = new classes_objects();        //Another object 
    System.out.println("X value for first object: " + object.x);
    System.out.println("X value for 2nd object: " + object2.x);

    object.x = 5;        //Changed 
    System.out.println("Changed value of x: " + object.x);
    //object.y = 10;        //WIll give error 
  }
}
